{
  "name": "proshop-app",
  "version": "1.0.0",
  "description": "A MERN Stack E-Commerce Application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server --delay 1",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haaaziq/eCommerce_ProShop.git"
  },
  "keywords": [
    "MERN",
    "Mongo",
    "Express",
    "ReactJs",
    "NodeJs",
    "FullStack",
    "WebApp",
    "e-commerce",
    "mongoose"
  ],
  "author": "haaaziq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haaaziq/eCommerce_ProShop/issues"
  },
  "homepage": "https://github.com/haaaziq/eCommerce_ProShop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.2.4"
  },
  "devDependencies": {
    "concurrently": "^7.0.0"
  }
}
